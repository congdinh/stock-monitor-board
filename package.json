{
  "name": "stock-monitor-service",
  "version": "1.0.0",
  "description": "Monitor and analysis data stock on many stock boards",
  "main": "index.js",
  "license": "PRIVATE",
  "private": true,
  "scripts": {
    "start": "tsc && node dist/index.js",
    "build": "rimraf dist & tsc -p tsconfig.json",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "crawl:stock": "ts-node-dev --respawn --transpile-only src/schedules/crawlStockBoard.ts",
    "crawl:exchange": "ts-node-dev --respawn --transpile-only src/schedules/crawlListExchange.ts",
    "sync:sheets": "ts-node-dev --respawn --transpile-only src/schedules/syncSheets.ts",
    "up": "ts-node-dev --respawn --transpile-only src/schedules/up.ts",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "@apollo/federation": "^0.20.4",
    "apollo-datasource": "^0.7.2",
    "apollo-server": "^2.18.2",
    "apollo-server-cache-redis": "^1.2.2",
    "apollo-server-express": "^2.18.2",
    "compression": "^1.7.4",
    "dataloader": "^2.0.0",
    "date-fns": "^2.15.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "google-spreadsheet": "^3.1.15",
    "googleapis": "^71.0.0",
    "graphql": "^14.7.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-middleware": "^4.0.2",
    "graphql-redis-subscriptions": "^2.1.2",
    "graphql-shield": "^7.3.2",
    "graphql-tools": "^6.0.15",
    "graphql-type-json": "^0.3.2",
    "ioredis": "^4.17.3",
    "lodash.foreach": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.isplainobject": "^4.0.6",
    "mongoose": "^5.7.1",
    "mongoose-timestamp": "^0.6.0",
    "puppeteer": "^5.3.1",
    "sift": "^13.3.0",
    "sleep": "^6.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/dotenv": "^6.1.1",
    "@types/google-spreadsheet": "^3.1.0",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/ioredis": "^4.0.18",
    "@types/lodash.foreach": "^4.5.6",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/mongoose": "5.5.12",
    "@types/node": "^14.0.18",
    "@types/puppeteer": "^3.0.2",
    "@types/sleep": "^0.0.8",
    "rimraf": "^3.0.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.50",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.9.6"
  }
}
